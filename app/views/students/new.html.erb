<h1> New Student </h1>

<form action="<%= posts_path %>" method="POST">
  
  <label>Student First Name:</label><br>
  <input id="first_name" name="student[first_name"><br>
  
  <label>Student Last Name:</label><br>
  <input id="last_name" name="student[last_name]"><br>
  
  <input type="submit" value="Submit Student">
   
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
  
  
</form>