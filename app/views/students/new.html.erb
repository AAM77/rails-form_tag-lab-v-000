<h1>Student Form </h1>

<%= form_tag students_path do %>
  <label>Student's First Name:</label><br>
  <%= text_field_tag :'student[first_name]' %><br>
  
  <label>Student's Last Name:</label><br>
  <%= text_field_tag :'student[last_name]' %><br>
  
  <%= submit_tag 'Submit Student' %><br>

<% end %>

<%= form_tag posts_path do %>
  
  <label>Post title:</label><br>
  <%= text_field_tag :'post[title]' %><br>
  
  <label>Post description:</label><br>
  <%= text_area_tag :'post[description]' %><br>
  
  <%= submit_tag "Submit Post" %>
  
<% end %>